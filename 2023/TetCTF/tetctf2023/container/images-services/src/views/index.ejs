    <script>
        $(function () {
            $('form').on('submit', function (e) {
                e.preventDefault();
                $("#error").html("")
                let url = $('#url').val()
                $.ajax({
                    type: 'POST',
                    url: '/api/getImage',
                    data: `url=${url}`,
                    success: function (data) {
                        if (!data.status) {
                            let html = `<div class="alert alert-danger" role="alert">${data.data}</div>`
                            $("#error").html(html)
                        }
                        else {
                            $("#img").attr('src', `data:image/png;base64,${data.data}`)
                        }
                    }, error: function (request, status, error) {
                        let html = `<div class="alert alert-danger" role="alert">${request.responseText}</div>`
                        $("#error").html(html)
                    }
                });

            });

        });
    </script>
